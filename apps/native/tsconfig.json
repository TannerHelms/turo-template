{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@views/*": [
        "app/views/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
      "@ui/*": [
        "app/ui/*"
      ],
    },
  }
}