{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "tailwind.config": [
        "./tailwind.config.js"
      ],
      "@ui/*": [
        "app/ui/*"
      ],
      "@views/*": [
        "app/views/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
      "@components/*": [
        "app/components/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}